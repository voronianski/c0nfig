{
  "name": "c0nfig",
  "version": "1.0.5",
  "description": "require local configs as if it's node_modules",
  "main": "./lib/index.js",
  "scripts": {
    "build": "babel src/index.js --out-file lib/index.js",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/voronianski/c0nfig.git"
  },
  "keywords": [
    "config",
    "configuration",
    "node_modules",
    "local",
    "require",
    "import",
    "umd"
  ],
  "author": "Dmitri Voronianski <dmitri.voronianski@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/voronianski/c0nfig/issues"
  },
  "homepage": "https://github.com/voronianski/c0nfig#readme",
  "prettier": {
    "singleQuote": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "husky": "^1.1.2",
    "prettier": "^1.14.3",
    "pretty-quick": "^1.8.0"
  }
